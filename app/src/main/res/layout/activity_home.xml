<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home"
    android:background="@drawable/back1">

    <Button
        android:id="@+id/buttonprofile"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:background="@drawable/main_button_choose"
        android:onClick="clickprofile"
        android:text="Profile"
        android:textColor="@color/design_default_color_secondary_variant"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.213" />

    <Button
        android:id="@+id/buttonvertualcheck"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:background="@drawable/main_button_choose"
        android:onClick="clickheathcheckup"
        android:text="Health Checkp"
        android:textColor="@color/design_default_color_secondary_variant"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonprofile"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonprofile"
        app:layout_constraintTop_toBottomOf="@+id/buttonprofile"
        app:layout_constraintVertical_bias="0.159" />

    <Button
        android:id="@+id/buttonprofile3"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:background="@drawable/main_button_choose"
        android:text="Prescription"
        android:textColor="@color/design_default_color_secondary_variant"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonvertualcheck"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonvertualcheck"
        app:layout_constraintTop_toBottomOf="@+id/buttonvertualcheck"
        app:layout_constraintVertical_bias="0.213" />

    <Button
        android:id="@+id/buttonprofile4"
        android:layout_width="285dp"
        android:layout_height="52dp"
        android:background="@drawable/main_button_choose"
        android:text="Appoint Doctor"
        android:textColor="@color/design_default_color_secondary_variant"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonprofile3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonprofile3"
        app:layout_constraintTop_toBottomOf="@+id/buttonprofile3"
        app:layout_constraintVertical_bias="0.316" />

</androidx.constraintlayout.widget.ConstraintLayout>